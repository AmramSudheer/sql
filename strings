select first_name,length(first_name) from employee_demographics;

select first_name,upper(first_name) from employee_demographics;


select first_name,left(first_name,3) as lef from employee_demographics;

select first_name,right(first_name,3) as lef from employee_demographics;

select first_name,substring(first_name,3,2) as lef from employee_demographics;


select first_name,replace(first_name,'a','z') as lef from employee_demographics;

select locate('x','alexander');

select first_name,last_name,concat(first_name,' ',last_name) as lef from employee_demographics;
